using System;
class Program {
    static bool EhPrimo(int n){
        int div=1;
        for(int i=2; i<n; i++){
            if(n%i==0){
            div++;
            return false;
            }
        }
        return true;
    }
    
  static void Main() {
      int n;
      bool prim;
      Console.WriteLine("entre com algum valor inteiro: ");
      n=int.Parse(Console.ReadLine());
      prim=EhPrimo(n);
      Console.WriteLine(prim);
      
    
  }
}
