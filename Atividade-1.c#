using System;
class Program
{
    static float soma(float n1, float n2, float n3)
    {
        float res = (n1 + n2 + n3) / 3;
        return res;
    }
    static void Main()
    {
        float num1 = 0, num2 = 0, num3 = 0, resposta = 0;
        num1 = int.Parse(Console.ReadLine());
        num2 = int.Parse(Console.ReadLine());
        num3 = int.Parse(Console.ReadLine());

        resposta = soma(num1, num2, num3);
        Console.WriteLine(resposta);
    }
}
